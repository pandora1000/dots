set completion-group-bg         "#000000"
set completion-group-fg         "#FFFFFF"
set completion-bg               "#000000"
set completion-fg               "#FFFFFF"
set completion-highlight-bg     "#ea6962"
set completion-highlight-fg     "#000000"

set selection-clipboard clipboard


set notification-error-bg       "#000000"
set notification-error-fg       "#FFFFFF"
set notification-warning-bg     "#000000"
set notification-warning-fg     "#FFFFFF"
set notification-bg             "#3c3836"
set notification-fg             "#FFFFFF"

set index-bg                    "#1d2021"
set index-fg                    "#FFFFFF"
set index-active-bg             "#282828"
set index-active-fg             "#FFFFFF"

set inputbar-bg                 "#000000"
set inputbar-fg                 "#FFFFFF"

set statusbar-bg                "#000000"
set statusbar-fg                "#FFFFFF"

set highlight-color             "#ea6962"
set highlight-active-color      "#ea6962"

set default-bg                  "#000000"
set default-fg                  "#FFFFFF"
set recolor-lightcolor          "#000000"
set recolor-darkcolor           "#666d6b"

set sandbox none
set statusbar-h-padding 0
set statusbar-v-padding 0
set page-padding 1
set selection-clipboard clipboard
map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map r reload
map R rotate
map K zoom in
map J zoom out
map i recolor
map p print
set scroll-step 80
map g goto top
set recolor                     true
